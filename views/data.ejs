<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <script src="/javascripts/vue.js"></script>
    <script src="/javascripts/vue-resource.js"></script>
    
    <!-- Bootstrap -->
    <link href="http://cdn.bootcss.com/bootstrap/3.3.6/css/bootstrap.css" rel="stylesheet">
    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.0/css/bootstrap-theme.css">

    <link rel='stylesheet' href='/stylesheets/datastyle.css' />

    <!-- this is uesd when the project is over -->
    <!-- <script src="/javascripts/vue.min.js"></script> -->
</head>
<body>

<!--导航栏-->
    <!-- Fixed navbar -->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          </button>
          <a class="navbar-brand" href="/">PM 2.5</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class= "active"><a href="/data">Search</a></li>
            <li><a href="/map">Map</a></li>
            <li><a href="#">About us</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>


    <div id="selector">
            <input type="text" placeholder="时间" name="username" v-model.number="time"/>
            <input type="text" placeholder="数量" name="username" v-model.number="quantity"/>
            <input type="button" value="search" style="height=20px;width=100px;" v-on:click="searchData"/>
    </div>
    <div>
      <table id="showList" class="table table-striped">
        <thead>
          <tr>
            <th>Lng</th>
            <th>Lat</th>
            <th>PM2.5</th>
          </tr>
        </thead>
        <tbody>
          <tr v-for="item in items">
            <td>{{item.LOT}}</td>
            <td>{{ item.LAT }}</td>
            <td>{{ item.PM }}</td>
          </tr>
        </tbody>
      </table>
    </div>



    <!-- this is the bind scripts -->
    <!-- 注意bind的js必须放在页面的最后 -->
    <script src="/javascripts/bind/data.js"></script>
    <script src="http://cdn.bootcss.com/jquery/1.11.1/jquery.min.js"></script>
    <script src="http://cdn.bootcss.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
  </body>
</html>
